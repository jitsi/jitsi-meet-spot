{
    "extends": ["@jitsi/eslint-config", "@jitsi/eslint-config/jsdoc", "@jitsi/eslint-config/react"],
    "overrides": [
        {
            "files": ["setupTests.js", "**/*.test.js"],
            "env": {
                "jest": true,
                "node": true
            }
        }
    ],
    "parser": "@babel/eslint-parser",
    "plugins": ["@babel", "import", "jest", "react"],
    "rules": {
        "no-console": 1
    },
    "settings": {
        "react": {
            "version": "detect"
        }
    }
}
